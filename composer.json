{
    "require": {
        "peanut/db": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "App\\Controllers\\": "app/controllers/",
            "App\\Models\\": "app/models/",
            "App\\Helpers\\Cores\\": "app/helpers/cores/",
            "App\\Traits\\": "app/traits/",
            "App\\Middlewares\\": "app/middlewares/",
            "Peanut\\Db\\": "vendor/peanut/db/"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url":  "https://github.com/yejune/peanut-database.git"
        }
    ],
    "require-dev": {
        "squizlabs/php_codesniffer": "*",
        "phing/phing": "*",
        "mayflower/php-codebrowser": "*"
    }
}
